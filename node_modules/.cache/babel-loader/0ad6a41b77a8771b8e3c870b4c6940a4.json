eturn null;\r\n            }\r\n        }).catch(() => {\r\n            return null\r\n        });\r\n}\r\n\r\nasync function editNews(news) {\r\n    return axios({\r\n        url: `${apiUrl}news`,\r\n        method: \"PUT\",\r\n        data: news.toJson(),\r\n        headers: {\"Content-Type\": 'application/json'},\r\n\r\n    })\r\n        .then(data => {\r\n            if (data.status == 200) {\r\n                return true;\r\n            } else {\r\n                return null;\r\n            }\r\n        }).catch(() => {\r\n            return null\r\n        });\r\n}\r\n\r\nasync function addNews(news) {\r\n    return axios({\r\n        url: `${apiUrl}news`,\r\n        method: \"POST\",\r\n        data: news.toJson(),\r\n        headers: {\"Content-Type\": 'application/json'},\r\n\r\n    })\r\n        .then(data => {\r\n            if (data.status == 200) {\r\n                return data.data.idNews;\r\n            } else {\r\n                return null;\r\n            }\r\n        }).catch(() => {\r\n            return null\r\n        });\r\n}\r\n"]}]}